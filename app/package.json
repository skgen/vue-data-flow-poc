{
  "name": "vue-3-csr-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "review": "yarn review:typescript && yarn review:eslint && yarn review:stylelint",
    "review:typescript": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "review:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "review:stylelint": "stylelint \"src/**/*.{scss,vue}\" --fix --color",
    "test:unit": "vitest --environment jsdom",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@patriarche/melkor": "~1.24.0",
    "@patriarche/vue-hooks": "^1.0.1",
    "@patriarche/vue-http": "^1.4.0",
    "@vueuse/core": "^10.4.1",
    "date-fns": "^2.29.2",
    "lodash": "^4.17.21",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@patriarche/eslint-config-vue": "^1.1.0",
    "@patriarche/stylelint-config-vue": "^1.1.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.0.0",
    "jsdom": "^22.0.0",
    "sass": "^1.52.3",
    "stylelint": "14.16.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.31.2",
    "vue-tsc": "^1.6.4"
  }
}
